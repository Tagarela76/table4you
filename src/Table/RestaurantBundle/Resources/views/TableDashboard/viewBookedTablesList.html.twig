
{% if activeTableList is not null %}
    {% for tableOrderList in activeTableList %}  
        {% if tableOrderList is not null %}
            <div class="row-fluid reserve-table-info">
                {% if tableOrderList.0 is defined %}
                    <div class="span10">
                        {{tableOrderList.0.tableNumber}}
                    </div>
                {% endif %}
                {% for tableOrder in tableOrderList %}  
                    <div class="span10">
                        <p>{{tableOrder.reserveDate|date('d.m.y')}} - {{tableOrder.reserveTime|date('H:i')}}</p>
                        <p>{{tableOrder.userName}}</p>
                        <p>{{tableOrder.userPhone}}</p>
                    </div>
                {% endfor %}
            </div>
        {% endif %}
    {% endfor %}
{% endif %}

{% if activeTableReservedByAdminList is not null %}
    {% for activeTable in activeTableReservedByAdminList %} 
        <div class="span10">
            <p>
                {{activeTable.tableNumber}} 
                {% trans %}main.tableMap.createMap.table{% endtrans %} - 
                {% trans %}main.tableMap.createMap.reservedByAdmin{% endtrans %}
            </p>
        </div>
    {% endfor %}
{% endif %}