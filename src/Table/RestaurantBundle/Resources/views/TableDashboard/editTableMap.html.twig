<div id="editTableMapPopup_{{tableMap.id}}" class="editTableMapPopup modal hide" data-backdrop="static">
    <div class="modal-header">
        <a class="close" data-dismiss="modal"></a>
    </div>
    <div class="modal-body" >
        <form id="edit-table-map-form_{{tableMap.id}}" action="{{ path('table_editTableMap') }}" enctype="multipart/form-data" method="post">       
            <div class="container-fluid">   
                <div class="row-fluid">
                    <span class="span12">
                        <div class="list-text-floor-hall">{% trans %}main.tableMap.createMap.form.label.loadMap{% endtrans %}</div>
                        <span class="btn btn-success btn-file">
                        <span>{% trans %}main.tableMap.tableList.form.label.selectTheFile{% endtrans %}</span>
                            <input type="file" name="file[]" id="image" size="1">
                        </span>
                    <span class="files-download"></span>
                    </span>
                    <span class="span4">
                        <div class="list-text-floor-hall">{% trans %}main.tableMap.createMap.form.label.floor{% endtrans %}</div>
                        <input type="text" name="mapFloor" size="2" value="{{tableMap.floor}}"/>
                    </span>
                    <span class="span5">
                        <div class="list-text-floor-hall">{% trans %}main.tableMap.createMap.form.label.hall{% endtrans %}</div>
                        <input type="text" name="mapHall" size="2" value="{{tableMap.hall}}"/>
                    </span>
                </div>  
                <div class="row-fluid">
                    <label class="validationError">{% trans %}main.tableMap.createMap.form.error.floor{% endtrans %}</label>
                    <div class="offset9 span3">
                        <input type="button" onClick="page.tableMap.validateEditForm(this);" value="{% trans %}main.tableMap.createMap.form.label.save{% endtrans %}" class="but-load-file"/>
                        <input type="hidden" name="tableMapId" value="{{tableMap.id}}" />
                    </div>   
                </div>    
            </div>
        </form>    
    </div>
</div>
