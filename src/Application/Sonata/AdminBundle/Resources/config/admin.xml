<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
      <service id="sonata.admin.restaurantCategory" class="Application\Sonata\AdminBundle\Controller\RestaurantCategoryAdmin">
          <tag name="sonata.admin" manager_type="orm" group="Restaurant" label="categories"/>
          <argument />
          <argument>Table\RestaurantBundle\Entity\RestaurantCategory</argument>
          <argument>SonataAdminBundle:CRUD</argument>
          <call method="setTranslationDomain">
              <argument>ApplicationSonataAdminBundle</argument>
          </call>
      </service>
      <service id="sonata.admin.restaurantKitchen" class="Application\Sonata\AdminBundle\Controller\RestaurantKitchenAdmin">
          <tag name="sonata.admin" manager_type="orm" group="Restaurant" label="kitchens"/>
          <argument />
          <argument>Table\RestaurantBundle\Entity\RestaurantKitchen</argument>
          <argument>SonataAdminBundle:CRUD</argument>
          <call method="setTranslationDomain">
              <argument>ApplicationSonataAdminBundle</argument>
          </call>
      </service>
      <service id="sonata.admin.restaurantAdditionalServices" class="Application\Sonata\AdminBundle\Controller\RestaurantAdditionalServicesAdmin">
          <tag name="sonata.admin" manager_type="orm" group="Restaurant" label="additionalServices"/>
          <argument />
          <argument>Table\RestaurantBundle\Entity\RestaurantAdditionalServices</argument>
          <argument>SonataAdminBundle:CRUD</argument>
          <call method="setTranslationDomain">
              <argument>ApplicationSonataAdminBundle</argument>
          </call>
      </service>
      <service id="sonata.admin.restaurant" class="Application\Sonata\AdminBundle\Controller\RestaurantAdmin">
          <tag name="sonata.admin" manager_type="orm" group="Restaurant" label="restaurants"/>
          <argument />
          <argument>Table\RestaurantBundle\Entity\Restaurant</argument>
          <argument>SonataAdminBundle:CRUD</argument>
          <call method="setTranslationDomain">
              <argument>ApplicationSonataAdminBundle</argument>
          </call>
      </service>
   </services>
</container>